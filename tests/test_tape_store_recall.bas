10 REM TEST TAPE STORE AND RECALL
20 DIM A(5)
30 FOR I = 0 TO 5
40 A(I) = I * 10
50 NEXT I
60 PRINT "ARRAY VALUES BEFORE STORE:"
70 FOR I = 0 TO 5
80 PRINT A(I);
90 NEXT I
100 PRINT
110 STORE A
120 PRINT "ARRAY STORED TO TAPE"
130 REM Clear array
140 FOR I = 0 TO 5
150 A(I) = 0
160 NEXT I
170 PRINT "ARRAY CLEARED:"
180 FOR I = 0 TO 5
190 PRINT A(I);
200 NEXT I
210 PRINT
220 RECALL A
230 PRINT "ARRAY RECALLED FROM TAPE:"
240 FOR I = 0 TO 5
250 PRINT A(I);
260 NEXT I
270 PRINT
280 END
