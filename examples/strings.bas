10 REM STRING MANIPULATION TEST
20 LET A$ = "HELLO"
30 LET B$ = "WORLD"
40 PRINT "STRING A = "; A$
50 PRINT "STRING B = "; B$
60 PRINT "CONCATENATION: "; A$ + " " + B$
70 PRINT "LENGTH OF A: "; LEN(A$)
80 PRINT "FIRST 3 CHARS OF A: "; LEFT$(A$, 3)
90 PRINT "LAST 2 CHARS OF B: "; RIGHT$(B$, 2)
100 PRINT "MID$ OF A (2,3): "; MID$(A$, 2, 3)
110 PRINT "ASCII OF 'H': "; ASC("H")
120 PRINT "CHR$(65) = "; CHR$(65)
130 END
